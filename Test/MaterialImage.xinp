<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<simulation>

  <geometry dim="3">
    <patchfile>EightCells.g2</patchfile>
    <topologysets>
      <set name="Model" type="volume">
        <item patch="1"/>
      </set>
      <set name="all" type="face">
        <item patch="1">1 2 3 4 5 6</item>
      </set>
    </topologysets>
  </geometry>

  <poroelasticity>
    <boundaryconditions>
      <dirichlet set="all" comp="123" />
    </boundaryconditions>
    <porotexturematerial set="Model">
      <property name="permx" file="permx.png" min="2.0" max="25.0"
                nx="40" ny="40" nz="40"/>
      <property name="poro" file="poro.png" min="0.09" max="0.28"
                nx="40" ny="40" nz="40"/>
      <property name="rhof" file="rho.png" min="1.0" max="8.0"
                nx="40" ny="40" nz="40"/>
    </porotexturematerial>
  </poroelasticity>

  <timestepping start="0" end="1" dt="1" />

  <discretization>
    <nGauss>2</nGauss>
  </discretization>

</simulation>
